export object fs{
	public static func readFile(string path) return %readFile(path);
	public static func fileExists(string path) return %fileExists(path);
	public static func writeFile(string path, string contents) %writeFile(path, contents);
	public static func removeFile(string path) return %deleteFile(path);
}