#mode "strict";

func getCharFromString(string str, int pos){
	return (int) str[pos];
}

func isLetter(char letter){
	int l = (int) letter;
	return (l >= 65 && l <= 90) || (l >= 97 && l <= 122);
}

func isNumber(char number){
	int n = (int) number;
	return n >= 48 && n <= 57;
}

