#mode "strict";

export object Int{
	static func Parse(string num){return (int)num;}
	static func Parse(char num){return (int)num;}
	static func IsInt(int a){return true;}
	static func IsInt(float a){return false;}
	static func IsInt(double a){return false;}
	static func IsInt(string a){return false;}
	func Int(int a){
		this.base = a;
	}
	private int base = 0;
}