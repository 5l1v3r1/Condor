import "console"

long YEAR = 31556926;
long MONTH = 2629743;
long WEEK = 604800;
long DAY = 86400;
long HOUR = 3600;

long BASE_YEAR = 1970;


string[] months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];

export object Date{
	private long base;
	private long decYear;
	private long decMonth;
	private int year;
	private int month;
	private int day;

	func Date(){
		this.base = %getTime();
		func toInt(var num) return (int) num;
		this.decYear = this.base / YEAR + 1970;
		this.year = toInt(this.decYear);
		this.decMonth = (this.decYear - this.year) * 12;
		this.month = toInt(this.decMonth) + 1;
		this.day = 10;
	}
	func Date(long time){
		this.base = time;
	}
	func toString(){
		return (string) this.base;
	}
	func getYear() {
		return this.year;
	}
	func toString(string format){
		func toInt(var num) return (int) num;
		string result = months[this.month] + " " + this.day + ", " + this.year;
		return result;
	}
}

Date date = new Date();
println(date.toString("s"));
