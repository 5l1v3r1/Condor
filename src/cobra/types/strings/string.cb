#mode "strict";

export object String{
	func String(){}
	func String(string str){
		this.base = str;
	}
	func String(int val){
		char v = (char) val;
		this.base = (string) v;
	}
	func String(char val){
		this.base = (string) val;
	}
	private func setSize(){
		int spot = -1;
		while (base[++spot]){
			length++;
		}
	}
	string base = "";
	int length = 0;
}