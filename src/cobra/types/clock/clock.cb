export object Clock{
	private int _clockId;
	func Clock(){
		this._clockId = %getClockId();
	}
	func start(){
		%startClock(this._clockId);
	}
	func stop(){
		%stopClock(this._clockId);
	}
	func getDuration(){
		return %getDuration(this._clockId);
	}
	static func getTime(){
		return %getTime();
	}
}